<h1 class="display-4 mb-4">
    add things
</h1>
<form action="/addContact" method="POST">
    <div class="mb-3">
        <label class="form-label">First name</label>
        <input name="firstname" type="text" class="form-control" placeholder="John">
    </div>
    <div class="mb-3">
        <label class="form-label">Last name</label>
        <input name="lastname" type="text" class="form-control" placeholder="Doe">
    </div>
    <div class="mb-3">
        <label class="form-label">Email</label>
        <input name="email" type="email" class="form-control" placeholder="johndoe@test.com">
    </div>
    <div class="mb-3">
        <label class="form-label">Age</label>
        <input name="age" type="number" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>